{
  "name": "puente_cloud",
  "version": "1.6.0",
  "description": "[Instructions](https://docs.back4app.com/docs/integrations/command-line-interface/setting-up-cloud-code-2/)",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "concurrently": "5.2.0",
    "esm": "3.2.25",
    "express": "4.17.1",
    "http": "0.0.0",
    "lodash": "4.17.19",
    "mailgun-js": "0.22.0",
    "parse": "1.11.1",
    "parse-dashboard": "2.1.0",
    "parse-server": "4.3.0",
    "parse-server-simple-mailgun-adapter": "1.0.1",
    "path": "0.12.7",
    "request": "2.88.0",
    "request-promise": "4.2.5",
    "simple2e": "1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/preset-env": "7.9.5",
    "@shelf/jest-mongodb": "1.1.5",
    "babel-jest": "25.4.0",
    "case": "1.6.3",
    "chai": "4.2.0",
    "chalk": "2.4.2",
    "codecov": "3.7.1",
    "env-cmd": "10.1.0",
    "eslint": "7.5.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "2.20.2",
    "fs": "0.0.1-security",
    "jasmine": "3.5.0",
    "jasmine-node": "3.0.0",
    "jest": "25.4.0",
    "mongodb": "3.5.8",
    "nodemon": "2.0.3",
    "pre-commit": "1.2.2",
    "randomstring": "1.1.5",
    "standard-version": "8.0.2",
    "supertest": "4.0.2"
  },
  "scripts": {
    "dashboard": "parse-dashboard --dev --appId myAppId --masterKey myMasterKey --serverURL http://localhost:1337/parse --appName PuenteCloudCode",
    "test": "env-cmd -f .env.dev jest",
    "test-staging": "env-cmd -f .env.staging jest && codecov",
    "start": "node -r esm index.js",
    "start-with-dash": "concurrently \"npm run start\" \"npm run dashboard\"",
    "start-with-db": "concurrently \"sudo mongod\" \"npm run start\"",
    "lint": "eslint '**/*.{js,jsx}' --quiet",
    "lint-fix": "eslint '**/*.{js,jsx}' --fix",
    "reinstall": "rm -rf node_modules/ && npm install",
    "gh-prune": "git fetch -p && for branch in `git branch -vv | grep ': gone]' | awk '{print $1}'`; do git branch -D $branch; done",
    "release": "standard-version",
    "snyk-protect": "snyk protect"
  },
  "engines": {
    "node": ">=4.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "preset": "@shelf/jest-mongodb",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hopetambala/puente-node-cloudcode.git"
  },
  "author": "Hope Tambala <hopetambala@puente-dr.com> (hopetambala.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hopetambala/puente-node-cloudcode/issues"
  },
  "homepage": "https://github.com/hopetambala/puente-node-cloudcode#readme"
}
